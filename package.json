{
  "name": "node-express-typescript",
  "version": "0.0.1",
  "description": "Node.js Express Structure for TypeScript Example",
  "main": "dist/server.js",
  "scripts": {
    "test": "jest",
    "lint": "tslint ./src/**/*.ts",
    "build": "rimraf dist && tsc",
    "prestart": "npm run build",
    "start": "node dist/server"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "debug": "^3.0.1",
    "express": "^4.15.2",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.2",
    "helmet": "^3.5.0",
    "lodash": "^4.17.4",
    "method-override": "^2.3.8",
    "morgan": "^1.8.1",
    "sequelize": "^4.8.3",
    "uuid": "^3.1.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.3",
    "@types/compression": "0.0.33",
    "@types/cookie-parser": "^1.3.30",
    "@types/csurf": "^1.9.33",
    "@types/debug": "0.0.30",
    "@types/express": "^4.0.35",
    "@types/express-handlebars": "0.0.29",
    "@types/express-session": "0.0.32",
    "@types/helmet": "0.0.34",
    "@types/jest": "^20.0.8",
    "@types/lodash": "^4.14.74",
    "@types/method-override": "0.0.29",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.13",
    "@types/sequelize": "^4.0.73",
    "@types/uuid": "^3.4.2",
    "@types/winston": "^2.3.5",
    "dtslint": "^0.1.2",
    "jest": "^21.1.0",
    "rimraf": "^2.6.1",
    "ts-jest": "^21.0.1",
    "tslint": "<5",
    "tslint-microsoft-contrib": "^4.0.1",
    "typescript": "^2.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/williamsdevaccount/node-express-typescript.git"
  },
  "keywords": [
    "TypeScript",
    "Node.js",
    "Nodejs",
    "Express"
  ],
  "homepage": "http://tested.co.kr",
  "author": "infiltrator@naver.com",
  "license": "MIT",
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.spec.(ts|js)"
    ],
    "testEnvironment": "node"
  }
}
